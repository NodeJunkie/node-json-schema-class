{
  "name": "json-schema-class",
  "version": "0.0.1",
  "description": "An Abstract Class that uses JSON Schema validation to build an extensible application with strongish type",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --collectCoverage",
    "docs": "node -r esm scripts/generate-readme.js && esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodeJunkie/node-json-schema-class.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "json-schema"
  ],
  "author": "Michael Feher <github@phearzero.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NodeJunkie/node-json-schema-class/issues"
  },
  "homepage": "https://github.com/NodeJunkie/node-json-schema-class#readme",
  "dependencies": {
    "ajv": "^6.12.2",
    "merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "acquit": "^1.0.5",
    "babel-jest": "^26.0.0",
    "chai": "^4.2.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "esm": "^3.2.25",
    "jest": "^26.0.0",
    "synk": "0.0.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "all": true
        }
      },
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "test": {
            "type": "mocha",
            "source": "./test",
            "includes": [
              "-test\\.(js)$"
            ]
          }
        }
      }
    ]
  }
}
