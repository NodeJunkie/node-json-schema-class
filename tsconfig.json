{
  "include": ["lib/**/*.js"],
  "typedocOptions": {
    "plugin": ["typedoc-plugin-coverage"],
    "entryPoints": ["./lib/SchemaClass.js"]
  },
  "compilerOptions": {
    "allowJs": true,
    "target": "es2016",
    "module": "commonjs",
     "declaration": true,
    // "declarationMap": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "skipLibCheck": true
  }
}
